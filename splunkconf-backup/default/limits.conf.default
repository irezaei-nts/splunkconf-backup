# we have to increase these limits or restore could fail in some conditions
# if these limits are too high for your env (ie consume too many ressource) or too low (you have bigger kvstore), please adjust
# we have to export these settings globally for them to be used by the restore endpoint
# see comments in splunkconf-restore script
[kvstore]
 max_documents_per_batch_save = 2000000
 max_rows_per_query = 2000000

